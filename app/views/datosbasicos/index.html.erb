<h1>Listing datosbasicos</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Seccion</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datosbasicos.each do |datosbasico| %>
      <tr>
        <td><%= datosbasico.titulo %></td>
        <td><%= datosbasico.seccion %></td>
        <td><%= datosbasico.descripcion %></td>
        <td><%= link_to 'Show', datosbasico %></td>
        <td><%= link_to 'Edit', edit_datosbasico_path(datosbasico) %></td>
        <td><%= link_to 'Destroy', datosbasico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datosbasico', new_datosbasico_path %>
