<!-- About Section -->
<section id="about">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading">Acerca de la base de datos</h2>
        <h3 class="section-subheading text-muted">Proyecto de Jaime H. Borja Gómez (jborja@uniandes.edu.co) </h3>
      </div>
    </div>
  <div class="row">
      <div class="col-lg-12">
        <ul class="timeline">
          <li>
            <div class="timeline-image">
              <img class="img-circle img-responsive" src="<%= image_url 'about/Proyecto.jpg' %>" alt="">
            </div>
            <div class="timeline-panel credits">
              <div class="timeline-heading">
                <h4>Proyecto</h4>
                <!--<h4 class="subheading">PROYECTO ARCA (Arte Colonial Americano)</h4>-->
              </div>
              <div class="timeline-body">
                <p class="text-muted"><%=@proyecto%></p>
              </div>
            </div>
            <div class="timeline-panel2 debits">
              <div class="timeline-heading">
                <h4 class="debit" align="left" >Propósito</h4>
                <!--<h4 class="subheading">An Agency is Born</h4>-->
              </div>
              <div class="timeline-body">
                <p class="debit" align="left" ><%=@proposito%></p>
              </div>
            </div>
          </li>
          <li>
            <div class="timeline-image">
              <img class="img-circle img-responsive" src="<%= image_url 'about/espacio.jpg' %>" alt="">
            </div>
            <div class="timeline-panel credits">
              <div class="timeline-heading">
                <h4>Espacio</h4>
                <!--<h4 class="subheading">Transition to Full Service</h4>-->
              </div>
              <div class="timeline-body">
                <p class="text-muted"><%=@espacio%></p>
              </div>
            </div>
            <div class="timeline-panel2 debits">
              <div class="timeline-heading">
                <h4 class="debit" align="left">El conjunto</h4>
                <!--<h4 class="subheading">Phase Two Expansion</h4>-->
              </div>
              <div class="timeline-body">
                <p class="debit" align="left"><%=@conjunto%></p>
              </div>
            </div>
          </li>
        </ul>
      </div>
    </div>
  </div>
</section>