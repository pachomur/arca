<h3>Listing artwork_symbols</h3>

<table class="table table-bordered table-condensed table-hover">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="list">
    <% @artwork_symbols.each do |artwork_symbol| %>
      <tr>
        <td><%= artwork_symbol.name %></td>
        <td><%= link_to 'Show', artwork_symbol %></td>
        <td><%= link_to 'Edit', edit_artwork_symbol_path(artwork_symbol) %></td>
        <td><%= link_to 'Destroy', artwork_symbol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= will_paginate @artwork_symbols,  renderer: BootstrapPagination::Rails %>
<br>

<%= link_to 'New Artwork symbol', new_artwork_symbol_path %>
